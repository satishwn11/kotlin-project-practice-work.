//8. Compare lengths of two lists.
fun main() {
    val list1 = listOf(1,3,3,83,9,28,9)
    val list2 = listOf(1,3,8,9,29,929,9,3)

    if(list1.size > list2.size) {
        println("list1 has a larger size: ${list1.size}")
    } else if(list2.size > list1.size) {
        println("list2 has a larger size: ${list2.size}")
    } else {
        println("Both list has the same size")
    }
}
